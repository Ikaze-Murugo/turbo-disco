# Laravel .gitignore

# Environment files
.env
.env.backup
.env.production
.env.local
.env.*.local
.env.example

# Composer dependencies
/vendor/

# Node.js dependencies
/node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Laravel storage
/storage/*.key
/storage/app/*
!/storage/app/.gitkeep
/storage/framework/cache/*
!/storage/framework/cache/.gitkeep
/storage/framework/sessions/*
!/storage/framework/sessions/.gitkeep
/storage/framework/views/*
!/storage/framework/views/.gitkeep
/storage/logs/*
!/storage/logs/.gitkeep

# Bootstrap cache
/bootstrap/cache/*

# Compiled assets
/public/build/
/public/hot
/public/storage
/storage/*.pdf

# Database
*.sqlite
*.sqlite3
database/*.sqlite

# IDE files
.vscode/
.idea/
*.swp
*.swo
*~

# OS files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Logs
*.log
logs/

# Coverage reports
/coverage
/.phpunit.result.cache

# Temporary files
*.tmp
*.temp
